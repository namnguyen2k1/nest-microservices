{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "ES2023",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true,
    "skipLibCheck": true,
    "strictNullChecks": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitAny": false,
    "strictBindCallApply": false,
    "noFallthroughCasesInSwitch": false,
    "paths": {
      "cache/cache": [
        "libs/cache/src"
      ],
      "cache/cache/*": [
        "libs/cache/src/*"
      ],
      "config/config": [
        "libs/config/src"
      ],
      "config/config/*": [
        "libs/config/src/*"
      ],
      "database/database": [
        "libs/database/src"
      ],
      "database/database/*": [
        "libs/database/src/*"
      ],
      "mail/mail": [
        "libs/mail/src"
      ],
      "mail/mail/*": [
        "libs/mail/src/*"
      ],
      "queue/queue": [
        "libs/queue/src"
      ],
      "queue/queue/*": [
        "libs/queue/src/*"
      ]
    }
  }
}